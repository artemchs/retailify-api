{
  "editor.fontSize": 14,
  "editor.fontFamily": "'JetBrainsMonoNL Nerd Font Mono', monospace",
  "files.autoSave": "afterDelay",
  "editor.tabSize": 4,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 1,
  "conventionalCommits.scopes": [
    "goods-receipts"
  ]
}
